{
  "name": "@wattanx/hooper-vue3",
  "version": "0.2.0",
  "type": "module",
  "description": "A customizable accessible carousel slider optimized for Vue 3",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./css": {
      "types": "./src/css.d.ts",
      "import": "./src/index.css",
      "require": "./src/index.css"
    }
  },
  "files": [
    "dist",
    "src/*.d.ts",
    "src/styles/*",
    "src/index.css"
  ],
  "scripts": {
    "test": "vitest run",
    "dev": "pnpm run build && pnpm -C playground dev",
    "build": "tsdown",
    "build:watch": "tsdown --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wattanx/hooper-vue3.git"
  },
  "author": "wattanx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wattanx/hooper-vue3/issues"
  },
  "homepage": "https://github.com/wattanx/hooper-vue3#readme",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.0.11",
    "prettier": "^3.3.2",
    "rollup": "^4.5.2",
    "rollup-plugin-postcss": "^4.0.2",
    "tsdown": "0.19.0-beta.1",
    "typescript": "^5.3.2",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "vue": "^3.3.8"
  },
  "dependencies": {
    "tiny-emitter": "^2.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.13.1"
}
