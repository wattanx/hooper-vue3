{
  "name": "@wattanx/hooper-vue3",
  "version": "0.0.5",
  "type": "module",
  "description": "A customizable accessible carousel slider optimized for Vue 3",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./css": {
      "import": "./src/index.css",
      "require": "./src/index.css"
    }
  },
  "files": [
    "dist",
    "src/*.d.ts",
    "src/styles/*",
    "src/index.css"
  ],
  "scripts": {
    "test": "vitest run",
    "dev": "pnpm -C playground dev",
    "build": "unbuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wattanx/hooper-vue3.git"
  },
  "author": "wattanx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wattanx/hooper-vue3/issues"
  },
  "homepage": "https://github.com/wattanx/hooper-vue3#readme",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/test-utils": "^2.4.2",
    "happy-dom": "^12.10.3",
    "rollup": "^4.5.2",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.3.2",
    "unbuild": "^2.0.0",
    "vite": "^5.0.2",
    "vitest": "^0.34.6",
    "vue": "^3.3.8"
  },
  "dependencies": {
    "tiny-emitter": "^2.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
